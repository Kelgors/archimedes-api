{
  "name": "api",
  "version": "1.0.0",
  "description": "",
  "main": "dist/src/index.js",
  "scripts": {
    "start": "node dist/index.ts",
    "start:dev": "nodemon",
    "build": "tsc",
    "db:migration:generate": "typeorm-ts-node-commonjs migration:generate -d src/db.ts",
    "db:migration:run": "typeorm-ts-node-commonjs migration:run -d src/db.ts",
    "db:migration:revert": "typeorm-ts-node-commonjs migration:revert -d src/db.ts",
    "db:seed": "ts-node db/seed.ts",
    "db:reset": "rm -f $DB_NAME && npm run db:seed",
    "db:reset:dev": "dotenv -e .env.development -- npm run db:reset",
    "db:reset:test": "dotenv -e .env.test -- npm run db:reset",
    "test": "npm run db:reset:test && nyc jest"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.6",
    "@types/lodash": "^4.14.200",
    "@types/node": "^20.8.9",
    "@types/supertest": "^2.0.15",
    "nodemon": "^3.0.1",
    "nyc": "^15.1.0",
    "pino-pretty": "^10.2.3",
    "prettier": "^3.0.3",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "argon2": "^0.31.1",
    "dotenv": "^16.3.1",
    "dotenv-cli": "^7.3.0",
    "express-jwt": "^8.4.1",
    "fastify": "^4.24.3",
    "fastify-plugin": "^4.5.1",
    "fastify-type-provider-zod": "^1.1.9",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^5.1.6",
    "typeorm": "^0.3.17",
    "zod": "^3.22.4"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
